<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/selector_white"
    android:padding="10dp"
    >
   <LinearLayout
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:id="@+id/ll_zizhi"
       >
    <ImageView
        android:layout_width="3dp"
        android:layout_height="fill_parent"
        android:background="@drawable/btn_lucency_green"
        />
       <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textSize="@dimen/txt_16sp"
           android:textColor="@color/txt_content_color"
           android:text="咨询师资质"
           android:layout_marginLeft="5dp"
           />
   </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_below="@+id/ll_zizhi"
        android:layout_marginTop="10dp"
        android:id="@+id/ll_biaoqian"
        >
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:id="@+id/ll_one"
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                >
               <ImageView
                   android:layout_width="20dp"
                   android:layout_height="20dp"
                   android:background="@mipmap/credential"
                   />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_one"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:id="@+id/ll_two"
                >
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@mipmap/credential"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_two"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:id="@+id/ll_three"
                >
               <ImageView
                   android:layout_width="20dp"
                   android:layout_height="20dp"
                   android:background="@mipmap/credential"
                   />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_three"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:id="@+id/ll_four"
                >
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@mipmap/credential"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_four"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp"
            android:visibility="gone"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:id="@+id/ll_five"

                >
               <ImageView
                   android:layout_width="20dp"
                   android:layout_height="20dp"
                   android:background="@mipmap/credential"
                   />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_five"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:id="@+id/ll_six"
                >
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@mipmap/credential"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/txt_14sp"
                    android:textColor="@color/txt_content_color"
                    android:id="@+id/tv_six"
                    android:text="心理咨询师"
                    android:layout_marginLeft="5dp"
                    />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ll_shan"
        android:layout_below="@+id/ll_biaoqian"
        android:layout_marginTop="10dp"
        >
        <ImageView
            android:layout_width="3dp"
            android:layout_height="fill_parent"
            android:background="@drawable/btn_lucency_green"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/txt_16sp"
            android:textColor="@color/txt_content_color"
            android:text="擅长方向"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/ll_shan"
        android:id="@+id/ll_begoodat"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/begoodat_one"
            android:layout_marginRight="10dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/begoodat_two"
            android:layout_marginRight="10dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/begoodat_three"
            android:layout_marginRight="10dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/jn"
        android:layout_below="@+id/ll_begoodat"
        android:layout_marginTop="10dp"
        >
        <ImageView
            android:layout_width="3dp"
            android:layout_height="fill_parent"
            android:background="@drawable/btn_lucency_green"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/txt_16sp"
            android:textColor="@color/txt_content_color"
            android:text="擅长技能"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/jn"
        android:id="@+id/ll_jn"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/jn_one"
            android:layout_marginRight="10dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/jn_two"
            android:layout_marginRight="10dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="情绪压力"
            android:textSize="@dimen/txt_14sp"
            android:textColor="@color/txt_content_color"
            android:background="@drawable/btn_yellow"
            android:padding="5dp"
            android:id="@+id/jn_three"
            android:layout_marginRight="10dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/intro"
        android:layout_below="@+id/ll_jn"
        android:layout_marginTop="10dp"
        >
        <ImageView
            android:layout_width="3dp"
            android:layout_height="fill_parent"
            android:background="@drawable/btn_lucency_green"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/txt_16sp"
            android:textColor="@color/txt_content_color"
            android:text="简介"
            android:layout_marginLeft="5dp"
            />
    </LinearLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/txt_annotation_color"
        android:textSize="@dimen/txt_14sp"
        android:id="@+id/tv_intro"
        android:layout_below="@+id/intro"
        android:layout_marginTop="10dp"
        android:text="@string/question_two"
        />
</RelativeLayout>